  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "mongoose": "^5.3.11"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
